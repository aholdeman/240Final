#include <iostream>

int main(int argc, char** argv) {

if (argc != 2) {
	cout << "Usage: " <<< argv[0] << "inputfile.txt";
	exit(1);
}

in_file.open(argv[1]);

if(in_file.fail()){
	cout << "Error opening the file" << argv[1] << "\n";
	exit(1);
}

String sequences[1000];
int sequenceIndex = 0;
while(!in_file.eof()) { #while the scanner is not at the end of the file
	getline(in_file, Input); #reads line and assigns it to the string Input
	sequences[sequenceIndex] = Input;
	sequenceIndex++;
}

in_file.close(); #closes input file to prevent memory leak

while(sequenceIndex >= 0) {
	cout << sequences[sequenceIndex];
	sequenceIndex--;
}
